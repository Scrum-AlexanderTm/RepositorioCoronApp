    function Obtener(id) {
        $.get("/Triaje/ListarKey?pCodigoTriaje=" + id, {}, function (data) {
            console.log(data.ObjTriaje);

            var item = data.ObjTriaje;
            var x;
            var y = item.Nombres;
            var z = item.NumeroDocumento ;
